{
  "name": "production-pr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "start:dev": "concurrently \"yarn start\" \"yarn start:dev:server\"",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "build": "webpack --env mode=development",
    "build:production": "webpack --env mode=production apiUrl=https://productionprjsonserver-bp1ti2ov.b4a.run/",
    "analyze:dev": "webpack --env mode=development analyze=true",
    "analyze:prod": "webpack --env mode=production analyze=true",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build:storybook": "storybook build -c ./config/storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "yarn build:storybook && loki --requireReference --reactUri file:./storybook-static",
    "test:ui:approve": "npx loki approve",
    "test:ui:report": "yarn test:ui:json && yarn test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "repository": "git@github.com:aizenka/production-pr.git",
  "author": "aizenka",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "1.8.0",
    "axios": "^1.6.2",
    "i18next": "21.6.11",
    "i18next-browser-languagedetector": "6.1.3",
    "i18next-http-backend": "1.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.15.5",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-webpack5": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-loader": "8.2.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "6.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-i18next": "5.1.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "json-server": "^0.17.4",
    "loki": "^0.33.1",
    "mini-css-extract-plugin": "2.5.3",
    "reg-cli": "^0.17.7",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "storybook": "^7.6.4",
    "style-loader": "3.3.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "ts-loader": "9.2.6",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.7.4"
  },
  "resolutions": {
    "@types/react": "17.0.39",
    "string-width": "^1.0.2 || 2 || 3 || 4"
  },
  "loki": {
    "fetchFailIgnore": ".*",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
